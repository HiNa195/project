<%- include("_header") -%>

<h1>Customer Update</h1>

<form action="/edit/<%= projCus.cusid %>" method="post">
  <div class="form-horizontal">

    <%- include("_editor") -%>

    <div class="form-group row mb-3">
      <label class="col-form-label col-sm-2"></label>
      <div class="col-sm-10">
        <input type="submit" value="Update Customer" class="btn btn-default btn-warning" />
      </div>
    </div>
  </div>
</form>

<% if (typeof success !== "undefined" && success !== null) { %>
  <div class="alert alert-success" role="alert">
    <%= success %>
  </div>
<% } %>

<%- include("_footer") -%>